/* Transition Variables */
:root {
  --primary-color: #003399;
  --secondary-color: #FF7A00;
  --success-color: #28a745;
  --warning-color: #ffc107;
  --info-color: #17a2b8;
  --error-color: #dc3545;
  --text-color: #333333;
  --heading-color: #495057;
  --white-color: #FFFFFF;
  --black-color: #000000;
  --bg-color: #f1f1f1;
  --card-color: #e7e7e7;
  --border-color: #D1D1D1;
  --input-bg-color: #FFFFFF;
  --input-text-color: #333333;
  --border-radius-small: 4px;
  --border-radius-medium: 8px;
  --border-radius-large: 16px;
  --all-transition: all 0.3s ease-in-out;
  --btn-transition: background-color 250ms cubic-bezier(0.4, 0, 0.2, 1) 0ms, box-shadow 250ms cubic-bezier(0.4, 0, 0.2, 1) 0ms, border-color 250ms cubic-bezier(0.4, 0, 0.2, 1) 0ms, color 250ms cubic-bezier(0.4, 0, 0.2, 1) 0ms;
  --box-shadow: 0px 2px 1px -1px rgba(0, 0, 0, 0.2), 0px 1px 1px 0px rgba(0, 0, 0, 0.14), 0px 1px 3px 0px rgba(0, 0, 0, 0.12);
}

/* Dark Mode */
[data-theme=dark] {
  --primary-color: #FF7A00;
  --secondary-color: #003399;
  --text-color: #828690;
  --heading-color: #ced4da;
  --bg-color: #0a0a0a;
  --card-color: #14171c;
  --border-color: #2d2d2d;
  --input-bg-color: #333333;
  --input-text-color: #E0E0E0;
}

* {
  box-sizing: border-box;
  padding: 0;
  margin: 0;
}

html,
body {
  max-width: 100vw;
}

body {
  color: var(--text-color);
  background: var(--bg-color);
  font-family: var(--font-vazir), sans-serif;
  font-size: 1rem;
  font-weight: 400;
  line-height: 1.5;
  direction: rtl;
  text-align: right;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

a {
  color: inherit;
  text-decoration: none;
}

h1, h2, h3, h4, h5, h6 {
  font-family: var(--font-vazir-bold);
  color: var(--heading-color);
}

ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

img {
  max-width: 100%;
  height: auto;
}

.om-container {
  width: 85%;
  max-width: 1600px;
  margin: 0 auto;
  padding: 0 15px;
}

.no-data {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.om-progress {
  width: 100%;
  height: 300px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.om-card {
  width: 100%;
  padding: 15px;
  border-radius: var(--border-radius-medium);
  background: var(--card-color);
  box-shadow: var(--box-shadow);
}

button a {
  display: flex;
  align-items: center;
}

/* MUI OVERRIDE */
/* MUI OVERRIDE */
/* MUI OVERRIDE */
.MuiButton-icon.MuiButton-startIcon {
  margin-right: -4px;
  margin-left: 8px;
}

.MuiButton-icon.MuiButton-endIcon {
  margin-right: 8px;
  margin-left: -4px;
}

.MuiFormControlLabel-root.MuiFormControlLabel-labelPlacementEnd {
  margin-left: 16px;
  margin-right: -11px;
}

.MuiButton-root svg {
  margin-left: 5px;
}

.MuiChip-icon {
  margin-left: -6px !important;
  margin-right: 5px !important;
}

.MuiButtonGroup-firstButton {
  border-top-right-radius: 4px !important;
  border-bottom-right-radius: 4px !important;
  border-top-left-radius: 0 !important;
  border-bottom-left-radius: 0 !important;
  border-left: none !important;
}

.MuiButtonGroup-lastButton {
  border-top-right-radius: 0 !important;
  border-bottom-right-radius: 0 !important;
  border-top-left-radius: 4px !important;
  border-bottom-left-radius: 4px !important;
  border-left: 1px solid rgba(0, 0, 0, 0.12) !important;
}

.MuiToggleButtonGroup-firstButton {
  border-top-right-radius: 4px !important;
  border-bottom-right-radius: 4px !important;
  border-top-left-radius: 0 !important;
  border-bottom-left-radius: 0 !important;
  border-left: none !important;
}

.MuiToggleButtonGroup-lastButton {
  border-top-right-radius: 0 !important;
  border-bottom-right-radius: 0 !important;
  border-top-left-radius: 4px !important;
  border-bottom-left-radius: 4px !important;
  border-left: 1px solid rgba(0, 0, 0, 0.12) !important;
}

.MuiButtonGroup-middleButton {
  border-right: 0 !important;
}

.MuiTablePagination-spacer {
  display: none;
}

.MuiTablePagination-input,
.MuiTablePagination-displayedRows,
.MuiTablePagination-selectLabel {
  font-family: var(--font-vazir) !important;
}

.MuiTablePagination-input {
  margin-right: 8px !important;
  margin-left: 32px !important;
}

.MuiTablePagination-toolbar {
  padding-left: 2px !important;
  padding-right: 24px !important;
  width: 100%;
  justify-content: center;
}

.MuiSwitch-switchBase {
  right: 0;
  left: auto !important;
}

.MuiSwitch-switchBase.Mui-checked {
  right: auto;
  left: 0 !important;
  transform: translateX(0px) !important;
}

.MuiDialog-paperFullScreen {
  width: 100% !important;
  max-width: 100% !important;
}

.MuiTab-root > .MuiTab-icon {
  margin-right: 0 !important;
  margin-left: 8px;
}

.MuiStepLabel-root.MuiStepLabel-vertical {
  text-align: right;
}
.MuiStepLabel-root.MuiStepLabel-vertical .MuiStepLabel-iconContainer {
  padding-left: 8px;
  padding-right: 0;
}
.MuiStepLabel-root.MuiStepLabel-vertical .MuiStepLabel-iconContainer svg {
  width: 2rem;
  height: 2rem;
  color: var(--secondary-color);
}

.MuiStepConnector-root.MuiStepConnector-vertical {
  margin-right: 15px;
  margin-left: 0;
}
.MuiStepConnector-root.MuiStepConnector-vertical .MuiStepConnector-line.MuiStepConnector-lineVertical {
  border-left-style: solid;
  border-left-width: 0;
  border-right-style: solid;
  border-right-width: 1px;
}

.MuiStepContent-root {
  margin-left: 0;
  margin-right: 15px;
  padding-left: 8px;
  padding-right: 20px;
  border-left: none !important;
  border-right: 1px solid #bdbdbd;
}

.MuiCollapse-root.MuiCollapse-vertical.MuiStepContent-transition {
  height: auto;
  visibility: visible;
}

.MuiStepLabel-label {
  display: flex !important;
  align-items: center;
  gap: 5px;
}

.MuiStepLabel-label.Mui-active {
  font-weight: 400 !important;
  color: inherit !important;
}

.MuiInputAdornment-positionEnd {
  margin-left: 0 !important;
  margin-right: 8px;
}

/* FORM */
.om-form {
  display: flex;
  flex-direction: column;
}

.om-form-control {
  width: 100%;
  margin-bottom: 10px !important;
}

.om-label {
  margin-bottom: 0.5rem;
  width: 100%;
  font-family: var(--font-vazir-bold);
  font-weight: 500;
}

input:focus-visible {
  outline: 1px solid var(--primary-color);
  box-shadow: none;
}

.om-input {
  width: 100%;
  height: 36px;
  padding: 8px;
  font-size: 0.875rem;
  background-color: var(--input-bg-color);
  color: var(--input-text-color);
  border-radius: var(--border-radius-medium);
  border: 1px solid var(--border-color);
  font-family: var(--font-vazir);
}

.om-input:disabled {
  cursor: default;
  background-color: #ccc;
  cursor: not-allowed;
}

.om-textarea {
  width: 100%;
  height: 120px;
  background: var(--white-color);
  padding: 8px;
  font-size: 0.875rem;
  background-color: var(--input-bg-color);
  color: var(--input-text-color);
  border-radius: var(--border-radius-medium);
  border: 1px solid var(--border-color);
  font-family: var(--font-vazir);
}
.om-textarea:focus-within {
  border-color: var(--primary-color);
  outline: var(--primary-color);
}

.om-password-field {
  position: relative;
  width: 100%;
  height: 40px;
  border-radius: var(--border-radius-medium);
}

.om-password-field .om-input {
  padding-left: 40px;
}

.om-password-field button {
  position: absolute;
  left: 5px;
}

.om-form-error {
  text-align: right !important;
  color: var(--error-color) !important;
  margin-right: 0 !important;
}

.om-switch-input {
  width: 100%;
  margin-left: 0 !important;
  margin-right: -11px !important;
}

.om-select {
  margin-top: 0 !important;
  height: 36px;
  background-color: var(--input-bg-color);
  color: var(--input-text-color);
  border-radius: var(--border-radius-medium);
  border: 1px solid var(--border-color);
}
.om-select .MuiNativeSelect-select {
  padding-left: 32px;
  padding-right: 5px !important;
  height: 36px;
}
.om-select .MuiNativeSelect-icon {
  left: 7px;
  right: auto;
}
.om-select::after {
  display: none;
}
.om-select::before {
  display: none;
}

.om-timepicker-container {
  display: flex;
  flex-direction: column;
  width: 100%;
}

.om-time-picker {
  width: 100%;
  margin-bottom: 10px !important;
  height: 36px;
}
.om-time-picker .MuiOutlinedInput-root {
  height: 36px;
}

.om-datepicker-container {
  display: flex;
  flex-direction: column;
  width: 100%;
}

.om-date-picker {
  width: 100%;
  margin-bottom: 10px !important;
  height: 36px;
}
.om-date-picker .MuiOutlinedInput-root {
  height: 36px;
}

.om-child-checkbox {
  margin-right: 30px;
}

input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
  -webkit-appearance: none;
  margin: 0;
}

/* HEADER */
.header {
  width: 100%;
}
.header .header-container {
  display: flex;
  width: 100%;
  padding-right: 30px;
  padding-left: 30px;
  height: 80px;
  justify-content: space-between;
  align-items: center;
  position: relative;
}
.header .header-container .header-menu {
  width: 40%;
  min-width: 40%;
  display: flex;
}
.header .header-container .left-menu {
  justify-content: flex-end;
}
.header .header-container .header-logo {
  width: 20%;
  min-width: 20%;
  margin-top: 10px;
  display: flex;
  justify-content: center;
}

.header-menu ul {
  display: flex;
  list-style: none;
  padding: 0;
  margin: 0;
}
.header-menu ul li {
  margin-left: 2rem;
}
.header-menu ul li a {
  color: var(--text-color);
  text-decoration: none;
  font-size: 1rem;
  line-height: 36px;
  font-weight: 500;
  transition: var(--all-transition);
  display: flex;
  align-items: center;
  gap: 5px;
}
.header-menu ul li a:hover {
  color: var(--primary-color) !important;
}

.header-util-button {
  max-width: 36px !important;
  min-width: 36px !important;
  width: 36px !important;
  color: var(--text-color) !important;
}
.header-util-button svg {
  margin-left: 0;
}

.header.front-header .header-container {
  padding-left: 0;
  padding-right: 0;
}
.header.front-header .header-menu {
  width: 100%;
}
.header.front-header .header-menu .header-logo {
  justify-content: flex-start;
  width: max-content;
  min-width: 60px;
  height: 80px;
  margin-top: 0;
  align-items: center;
  margin-left: 30px;
}
.header.front-header .header-menu .header-logo a {
  height: 80px;
  display: flex;
  align-items: center;
}
.header.front-header .header-menu ul li a {
  color: var(--white-color);
}
.header.front-header .header-menu ul li:hover {
  color: var(--primary-color) !important;
}
.header.front-header .header-menu ul li.menu-item.button-item button {
  color: #ffffff;
}
.header.front-header .header-menu ul .header-address {
  color: #ffffff;
}
.header.front-header .header-util-button {
  color: #ffffff !important;
}

.front-header {
  position: fixed;
  top: 0;
  height: 80px;
  z-index: 10;
}
.front-header .header-container .header-menu {
  align-items: center;
}

/* PAGE LAYOUT */
.inner-page {
  width: 100%;
  min-height: calc(100vh - 80px);
  height: 100%;
  overflow: auto;
}

.page-wrapper {
  width: 100%;
  margin-bottom: 40px;
  display: flex;
}
.page-wrapper .sidebar {
  width: 25%;
  min-width: 25%;
  margin-left: 2%;
}
.page-wrapper .content {
  width: 100%;
}

/*********** HOMEPAGE ***********/
.home-page {
  width: 100%;
  height: 100vh;
}
.home-page .full-bg-container {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100vh;
  background-position: center center;
  background-size: cover;
  background-repeat: no-repeat;
  position: relative;
}
.home-page .full-bg-container::before {
  content: "";
  width: 100%;
  height: 100vh;
  display: flex;
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
}
.home-page .full-bg-container {
  background-image: url("../public/assets/images/website/auth-bg-1.webp");
}
.home-page .full-bg-container::before {
  background-color: rgba(0, 0, 0, 0.7);
}
.home-page .home-page-wrapper {
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
  z-index: 1;
  position: relative;
}
.home-page .home-page-wrapper h1 {
  color: var(--white-color);
}
.home-page .home-page-wrapper h4 {
  color: var(--white-color);
}
.home-page .home-page-wrapper .home-login-form {
  width: 350px;
  margin-top: 20px;
}
.home-page .home-page-wrapper .home-login-form label {
  color: var(--white-color);
}
.home-page .home-page-wrapper .home-login-form .Mui-disabled {
  background-color: rgba(255, 255, 255, 0.12);
  color: var(--white-color);
}

/* AUTH PAGE */
.auth-page {
  width: 100%;
  height: 100vh;
}
.auth-page .full-bg-container {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100vh;
  background-position: center center;
  background-size: cover;
  background-repeat: no-repeat;
  position: relative;
}
.auth-page .full-bg-container::before {
  content: "";
  width: 100%;
  height: 100vh;
  display: flex;
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
}

.auth-page.client-auth .auth-box-wrapper {
  width: 100%;
  height: 100vh;
  display: flex;
  align-items: center;
  justify-content: space-around;
  text-align: center;
  z-index: 1;
  position: relative;
}
.auth-page.client-auth .auth-box-wrapper .auth-page-box {
  width: 350px;
}

.auth-page.admin-auth .full-bg-container {
  background-image: url("../public/assets/images/website/auth-bg-2.webp");
}
.auth-page.admin-auth .full-bg-container::before {
  background-color: rgba(0, 0, 0, 0.9);
}
.auth-page.admin-auth .auth-box-wrapper {
  width: 350px;
  height: max-content;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin: auto;
  background-color: var(--bg-color);
  padding: 20px;
  border-radius: var(--border-radius-medium);
  position: relative;
}
.auth-page.admin-auth .auth-box-wrapper .auth-page-box {
  width: 100%;
}
.auth-page.admin-auth .auth-box-wrapper h1 {
  text-align: center;
}
.auth-page.admin-auth .auth-box-wrapper h4 {
  text-align: center;
}

/* PANEL */
.panel-page-wrapper {
  min-height: 100vh;
  height: 100%;
  padding-right: 330px;
  background-color: var(--bg-color);
}
.panel-page-wrapper .panel-header {
  height: 80px;
  background-color: #fff;
  margin-bottom: 20px;
  border-bottom-right-radius: var(--border-radius-small);
  box-shadow: var(--box-shadow);
}
.panel-page-wrapper .panel-header .panel-header-container {
  max-width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 20px;
}
.panel-page-wrapper .panel-sidebar {
  background-color: #fff;
  box-shadow: var(--box-shadow);
  width: 300px;
  padding: 30px 30px;
  height: 100vh;
  display: flex;
  justify-content: center;
  position: fixed;
  top: 0;
  right: 0;
  transition: 0.35s;
  z-index: 10;
  overflow-y: auto;
}
.panel-page-wrapper .panel-content {
  padding-left: 30px;
  padding-bottom: 30px;
}
.panel-page-wrapper .panel-content .panel-inner-content {
  width: 100%;
  margin-top: 20px;
}

.panel-inner-content.form-content {
  background-color: #fff;
  padding: 20px;
  border-radius: var(--border-radius-medium);
  box-shadow: var(--box-shadow);
}

.panel-header-menu ul {
  list-style: none;
  margin: 0;
  padding: 0;
  display: flex;
  align-items: center;
  gap: 20px;
}
.panel-header-menu ul a {
  display: flex;
  align-items: center;
  gap: 5px;
}
.panel-header-menu ul a:hover {
  color: var(--primary-color);
}
.panel-header-menu ul .util-item button {
  width: 36px;
  max-width: 36px;
  min-width: 36px;
  height: 36px;
  border-radius: 50%;
  background: var(--bg-color);
  color: var(--text-color);
  padding: 0;
}

.panel-sidebar-container {
  height: 100%;
  width: 100%;
}
.panel-sidebar-container .panel-sidebar-top {
  display: flex;
  align-items: center;
  gap: 10px;
  padding-bottom: 10px;
}
.panel-sidebar-container .panel-sidebar-top .panel-sidebar-top-text {
  height: 50px;
  display: flex;
  flex-direction: column;
}
.panel-sidebar-container .panel-sidebar-top .panel-sidebar-top-text h6 {
  font-size: 16px;
  line-height: 24px;
  margin-bottom: 0;
}
.panel-sidebar-container .panel-sidebar-top .panel-sidebar-top-text span {
  font-size: 0.875rem;
  color: var(--primary-color);
}
.panel-sidebar-container .panel-sidebar-menu ul {
  list-style: none;
  padding: 0;
  margin: 0;
}
.panel-sidebar-container .panel-sidebar-menu ul .panel-sidebar-separator {
  margin-bottom: 10px;
  border-top: 1px solid var(--border-color);
  border-bottom: 1px solid var(--border-color);
  padding-bottom: 5px;
  padding-top: 5px;
}
.panel-sidebar-container .panel-sidebar-menu ul .panel-sidebar-separator h5 {
  margin-bottom: 0;
}
.panel-sidebar-container .panel-sidebar-menu ul li {
  margin-bottom: 10px;
  border-right: 3px solid transparent;
  padding-right: 10px;
  border-radius: var(--border-radius-small);
}
.panel-sidebar-container .panel-sidebar-menu ul li a {
  color: var(--text-color);
  text-decoration: none;
  font-weight: 500;
  transition: var(--all-transition);
  display: flex;
  align-items: center;
  gap: 5px;
}
.panel-sidebar-container .panel-sidebar-menu ul li a:hover {
  color: var(--primary-color) !important;
}
.panel-sidebar-container .panel-sidebar-menu ul li a span {
  font-weight: 600;
}
.panel-sidebar-container .panel-sidebar-menu ul li.active-menu {
  border-right: 3px solid var(--primary-color);
  background-color: rgba(0, 51, 153, 0.09);
}
.panel-sidebar-container .panel-sidebar-menu ul li.active-menu a {
  color: var(--primary-color) !important;
  font-family: var(--font-vazir-bold);
}

.panel-inner-header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 20px;
  border-right: 5px solid var(--primary-color);
  padding: 15px;
  border-radius: var(--border-radius-medium);
  background-color: #fff;
  box-shadow: var(--box-shadow);
}
.panel-inner-header h5 {
  font-weight: 600;
  margin-bottom: 5px;
}
.panel-inner-header .panel-inner-header-btns {
  display: flex;
  gap: 10px;
}

.panel-filter-drawer .MuiPaper-root {
  right: auto;
  left: 0;
}

.panel-filter-container {
  width: 300px;
  height: auto;
  min-height: 100vh;
  padding: 20px;
}
.panel-filter-container .panel-filter-header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid var(--border-color);
  padding-bottom: 10px;
  margin-bottom: 10px;
}

.filter-form button {
  width: 100%;
}

.panel-grid-one {
  display: grid;
  grid-template-columns: repeat(1, minmax(0, 1fr));
  gap: 20px;
  margin-bottom: 10px;
}

.panel-grid-two {
  display: grid;
  grid-template-columns: repeat(2, minmax(0, 1fr));
  gap: 20px;
}

.panel-grid-three {
  display: grid;
  grid-template-columns: repeat(3, minmax(0, 1fr));
  gap: 20px;
}

.panel-new-img-container {
  display: flex;
  align-items: flex-start;
  gap: 20px;
  margin-bottom: 20px;
  margin-top: 20px;
}
.panel-new-img-container .MuiPaper-root {
  width: 100%;
}
.panel-new-img-container > img {
  min-height: 80px;
}

.panel-table-image-wrapper {
  display: flex;
  justify-content: center;
}
